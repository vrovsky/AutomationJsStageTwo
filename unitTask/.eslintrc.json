{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": "off",
    "@quotes": "off",
    "comma-dangle": "off",
    "max-len": "off",
    "linebreak-style": 0,
    "space-before-function-paren": 0,
    "object-curly-spacing": 0
  }
}
